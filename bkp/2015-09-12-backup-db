  KEY `post_id` (`post_id`),
  KEY `meta_key` (`meta_key`(191))
) ENGINE=MyISAM AUTO_INCREMENT=11487 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci ;

